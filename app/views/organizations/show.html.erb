<%= render layout: "/shared/layout", locals: {
  subject: @organization,
  sidebar: true,
  datepicker: true,
  buttons: [
    generate_organization_report_button(@organization.id),
    layout_button(link: new_organization_path, icon: "fas fa-plus", title: "Add team", admin: true),
  ]
} do %>
  <small class="text-center d-block text-muted mb-2">
    please ensure ad blockers are disabled to view the overview
  </small>
  <iframe
    src="<%= @iframe_url %>"
    class="metabase-card"
    height="800"
    scrolling="yes"
    allowtransparency>
  </iframe>
<% end %>
