<%= render layout: "/campaigns/layout", locals: {campaign: @eventable} do %>
  <%= card do %>
    <%= card_header do %>
      <%= card_title "Events" %>
      <%= card_subtitle "List of events related to this campaign" %>
    <% end %>
    <%= card_body do %>
      <%= render "/events/table", eventable: @eventable, events: @events %>
    <% end %>
  <% end %>
<% end %>
