<%= render layout: "/properties/layout", locals: {property: @property} do %>
  <div class="section-block d-flex justify-content-between align-items-center mt-0 mb-3">
    <h1 class="section-title mb-0">Property Earnings</h1>
    <%= render "/shared/date_range_picker" %>
  </div>

  <%= card do %>
    <%= card_body do %>
      <%= render "/property_earnings/table", property: @property %>
    <% end %>
  <% end %>
<% end %>
