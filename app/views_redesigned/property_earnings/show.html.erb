<%= render layout: "/properties/layout", locals: { property: @property } do %>
  <%= card do %>
    <%= card_header do %>
      <%= card_title do %>
        Estimated Earnings
      <% end %>
      <%= card_subtitle do %>
        Estimated earnings by day for this property
      <% end %>
    <% end %>
    <%= card_body do %>
      <%= render "/property_earnings/table", property: @property %>
    <% end %>
  <% end %>
<% end %>
