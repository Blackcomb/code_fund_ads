<%= render layout: "/users/layout", locals: { user: @versionable } do %>
  <%= card do %>
    <%= card_header do %>
      <%= card_title do %>
        User Revisions
      <% end %>
      <%= card_subtitle do %>
        List of revisions to this user
      <% end %>
    <% end %>
    <%= card_body do %>
      <%= render "/versions/table", versionable: @versionable, versions: @versions %>
    <% end %>
  <% end %>
<% end %>
