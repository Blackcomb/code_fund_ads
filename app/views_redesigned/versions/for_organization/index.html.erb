<%= render layout: "/organizations/layout", locals: { organization: @versionable } do %>
  <%= card do %>
    <%= card_header do %>
      <%= card_title do %>
        Organization Revisions
      <% end %>
      <%= card_subtitle do %>
        List of revisions to this organization
      <% end %>
    <% end %>
    <%= card_body padding: "p-0" do %>
      <%= render "/versions/table", versionable: @versionable %>
    <% end %>
  <% end %>
<% end %>
