<%= render layout: "/properties/layout", locals: { property: @versionable } do %>
  <%= card do %>
    <%= card_header do %>
      <%= card_title do %>
        Property Revisions
      <% end %>
      <%= card_subtitle do %>
        List of revisions to this property
      <% end %>
    <% end %>
    <%= card_body do %>
      <%= render "/versions/table", versionable: @versionable, versions: @versions %>
    <% end %>
  <% end %>
<% end %>
